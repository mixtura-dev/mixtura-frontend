    "/api/auth/signup/verify": {
      "post": {
        "tags": ["auth"],
        "summary": "Verify Sign Up",
        "operationId": "verify_sign_up_api_auth_signup_verify_post",
        "requestBody": {
          "content": {
            "application/json": { "schema": { "$ref": "#/components/schemas/EmailVerifyRequest" } }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": { "schema": { "$ref": "#/components/schemas/VerifyResponse" } }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/HTTPValidationError" }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "BusyResponse": {
        "properties": {
          "status": { "type": "string", "title": "Status", "default": "ok" },
          "busy": { "type": "boolean", "title": "Busy", "default": true }
        },
        "type": "object",
        "title": "BusyResponse"
      },
      "EmailRequest": {
        "properties": { "email": { "type": "string", "format": "email", "title": "Email" } },
        "type": "object",
        "required": ["email"],
        "title": "EmailRequest"
      },
      "EmailVerifyRequest": {
        "properties": {
          "email": { "type": "string", "format": "email", "title": "Email" },
          "token": { "type": "string", "title": "Token" }
        },
        "type": "object",
        "required": ["email", "token"],
        "title": "EmailVerifyRequest"
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": { "$ref": "#/components/schemas/ValidationError" },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "OAuthConfirm": {
        "properties": {
          "provider": { "type": "string", "title": "Provider" },
          "code": { "type": "string", "title": "Code" }
        },
        "type": "object",
        "required": ["provider", "code"],
        "title": "OAuthConfirm"
      },
      "PasswordConfirmRequest": {
        "properties": {
          "email": { "type": "string", "format": "email", "title": "Email" },
          "token": { "type": "string", "title": "Token" },
          "password": { "type": "string", "title": "Password" },
          "repeat_password": { "type": "string", "title": "Repeat Password" }
        },
        "type": "object",
        "required": ["email", "token", "password", "repeat_password"],
        "title": "PasswordConfirmRequest"
      },
      "Provider": {
        "properties": {
          "icon_url": { "type": "string", "title": "Icon Url" },
          "id": { "type": "string", "title": "Id" },
          "display_name": { "type": "string", "title": "Display Name" },
          "redirect_uri": { "type": "string", "title": "Redirect Uri" },
          "use_in_auth": { "type": "boolean", "title": "Use In Auth" },
          "limit": { "type": "integer", "title": "Limit" }
        },
        "type": "object",
        "required": ["icon_url", "id", "display_name", "redirect_uri", "use_in_auth", "limit"],
        "title": "Provider"
      },
      "ProviderModel": {
        "properties": {
          "name": { "type": "string", "title": "Name" },
          "client_id": {
            "anyOf": [{ "type": "string" }, { "type": "null" }],
            "title": "Client Id"
          },
          "client_username": {
            "anyOf": [{ "type": "string" }, { "type": "null" }],
            "title": "Client Username"
          }
        },
        "type": "object",
        "required": ["name", "client_id", "client_username"],
        "title": "ProviderModel"
      },
      "Providers": {
        "properties": {
          "email_enabled": { "type": "boolean", "title": "Email Enabled" },
          "oauth_providers": {
            "items": { "$ref": "#/components/schemas/Provider" },
            "type": "array",
            "title": "Oauth Providers"
          }
        },
        "type": "object",
        "required": ["email_enabled", "oauth_providers"],
        "title": "Providers"
      },
      "SignInRequest": {
        "properties": {
          "login": { "type": "string", "title": "Login" },
          "password": { "type": "string", "minLength": 6, "title": "Password" }
        },
        "type": "object",
        "required": ["login", "password"],
        "title": "SignInRequest"
      },
      "SignupConfirmRequest": {
        "properties": {
          "email": { "type": "string", "format": "email", "title": "Email" },
          "username": { "type": "string", "title": "Username" },
          "token": { "type": "string", "title": "Token" },
          "password": { "type": "string", "title": "Password" },
          "repeat_password": { "type": "string", "title": "Repeat Password" }
        },
        "type": "object",
        "required": ["email", "username", "token", "password", "repeat_password"],
        "title": "SignupConfirmRequest"
      },
      "StatusResponse": {
        "properties": { "status": { "type": "string", "title": "Status", "default": "ok" } },
        "type": "object",
        "title": "StatusResponse"
      },
      "UpdateResponse": {
        "properties": {
          "status": { "type": "string", "title": "Status", "default": "ok" },
          "updated": { "type": "boolean", "title": "Updated", "default": true }
        },
        "type": "object",
        "title": "UpdateResponse"
      },
      "UserModel": {
        "properties": {
          "id": { "type": "string", "format": "uuid", "title": "Id" },
          "username": { "anyOf": [{ "type": "string" }, { "type": "null" }], "title": "Username" },
          "email": { "anyOf": [{ "type": "string" }, { "type": "null" }], "title": "Email" },
          "registration_date": {
            "type": "string",
            "format": "date-time",
            "title": "Registration Date"
          },
          "providers": {
            "items": { "$ref": "#/components/schemas/ProviderModel" },
            "type": "array",
            "title": "Providers"
          }
        },
        "type": "object",
        "required": ["id", "username", "email", "registration_date", "providers"],
        "title": "UserModel"
      },
      "UsernameRequest": {
        "properties": {
          "username": { "type": "string", "maxLength": 16, "minLength": 4, "title": "Username" }
        },
        "type": "object",
        "required": ["username"],
        "title": "UsernameRequest"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": { "anyOf": [{ "type": "string" }, { "type": "integer" }] },
            "type": "array",
            "title": "Location"
          },
          "msg": { "type": "string", "title": "Message" },
          "type": { "type": "string", "title": "Error Type" }
        },
        "type": "object",
        "required": ["loc", "msg", "type"],
        "title": "ValidationError"
      },
      "VerifyResponse": {
        "properties": {
          "status": { "type": "string", "title": "Status", "default": "ok" },
          "verified": { "type": "boolean", "title": "Verified", "default": false }
        },
        "type": "object",
        "title": "VerifyResponse"
      }
    }
  }
}
